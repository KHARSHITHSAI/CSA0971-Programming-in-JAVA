import java.util.Scanner;
class mnkskip {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter the starting value: ");
        int M = sc.nextInt();
        System.out.print("Enter the End value: ");
        int N = sc.nextInt();
        System.out.print("Enter the value to skip in between: ");
        int K = sc.nextInt();
        System.out.println("Numbers from " + M + " to " + N + " by skipping " + K + " numbers in between:");
        for (int i = M; i <= N; i+=(K+1)) {
            System.out.print(i + " ");
        }
    }
}
